<?xml version="1.0"?>
  <table name="tournament_series" type="normal">
  <columns>
    <column name="id"   type="number(12)"   nullable="n"/>
    <column name="name" type="varchar2(64)" nullable="n"/>
    <column name="code" type="varchar2(64)" nullable="n"/>
  </columns>
  <constraints>
    <constraint name="pk_tournament_series"  type="primary key" columns_list="id"/>
    <constraint name="unq_tournament_series" type="unique" columns_list="name"/>
  </constraints>
</table>
