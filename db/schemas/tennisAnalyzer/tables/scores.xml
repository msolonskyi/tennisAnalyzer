<?xml version="1.0"?>
  <table name="scores" type="normal">
  <columns>
    <column name="id"                           type="number(12)" nullable="n"/>
    <column name="delta_hash"                   type="number"     nullable="n"/>
    <column name="tournament_id"                type="number(12)"/>
    <column name="tournament_name"              type="varchar2(128)"/>
    <column name="tournament_code"              type="number(4)"/>
    <column name="tournament_url"               type="varchar2(256)"/>
    <column name="tournament_city"              type="varchar2(128)"/>
    <column name="tournament_country_code"      type="varchar2(128)"/>
    <column name="tournament_year"              type="number(4)"/>
    <column name="tournament_week"              type="number(2)"/>
    <column name="tournament_type_id"           type="number(12)"/>
    <column name="tournament_type_name"         type="varchar2(8)"/>
    <column name="tournament_surface_id"        type="number(12)"/>
    <column name="tournament_surface_name"      type="varchar2(8)"/>
    <column name="tournament_start_dtm"         type="date"/>
    <column name="tournament_series_id"         type="number(12)"/>
    <column name="tournament_series_name"       type="varchar2(64)"/>
    <column name="tournament_series_code"       type="varchar2(64)"/>
    <column name="tournament_prize_money"       type="number(10)"/>
    <column name="tournament_prize_currency"    type="varchar2(16)"/>
  </columns>
  <constraints>
    <constraint name="pk_scores" type="primary key" columns_list="id"/>
  </constraints>
</table>
