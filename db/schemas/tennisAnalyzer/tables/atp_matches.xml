    <constraint name="pk_atp_matches"               type="primary key" columns_list="id"/>
    <constraint name="unq_atp_matches"              type="unique"      columns_list="tournament_id, winner_code, loser_code, stadie_id"/>
    <constraint name="unq_atp_matches$stats_url"    type="unique"      columns_list="stats_url"/>
    <constraint name="fk_atp_matches$tournament_id" type="foreign key" columns_list="tournament_id" foreign_table="atp_tournaments" foreign_columns_list="id"/>
    <constraint name="fk_atp_matches$stadie_id"     type="foreign key" columns_list="stadie_id"     foreign_table="stadies"         foreign_columns_list="id"/>
    <constraint name="fk_atp_matches$winner_code"   type="foreign key" columns_list="winner_code"   foreign_table="atp_players"     foreign_columns_list="code"/>
    <constraint name="fk_atp_matches$loser_code"    type="foreign key" columns_list="loser_code"    foreign_table="atp_players"     foreign_columns_list="code"/>
  </constraints>
